#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules:/mnt/c/Users/chaie/OneDrive/Desktop/PFA/PFA/pfa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
else
  exec node  "$basedir/../../../../../../@angular+compiler-cli@21.0._314f3355ea68250383a9b8910ac4ceab/node_modules/@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
fi
